<check_type:"Unix">

<custom_item>
	system			:	"Linux"
	type			:	CMD_EXEC
	description		:	"Custom Asset Discovery Java"
	cmd				:	"for bar in `timeout 30s find / -not -path \"/proc/*\" -not -path \"*/docker/*\" -executable -type f  -size -1M  -maxdepth 8 -name java 2>/dev/null`; do $bar -version 2>&1; done"
	expect		:	"[Vv]ersion"
#	severity    	: 	MEDIUM
#	quiet			:	YES
	dont_echo_cmd	:	YES
</custom_item>

<custom_item>
	system			:	"Linux"
	type			:	CMD_EXEC
	description		:	"Custom Asset Discovery WebLogic"
	cmd				:	"cd /tmp && foo=`timeout 60s find / -not -path \"/proc/*\" -not -path \"*/docker/*\" -type f -size -10M  -maxdepth 8 -name \"registry.xml\" 2>/dev/null`; if [ $? -eq 124 ]; then echo \"WebLogic not found (Timeout)\"; else touch NessusCAD;  for bar in $foo; do grep  \"WebLogic Server\" $bar | awk -F \'=\' \'{ print $4 }\'| cut -c 2- | sed \'s/..$//\' >>NessusCAD; done; for bar in $foo; do grep -i \'component name=\"WebLogic Server\" Version\' $bar | awk -F \'\"\' \'{ print $4 }\' >>NessusCAD; done; for ver in `uniq NessusCAD 2>/dev/null`; do echo \"WebLogic Version: $ver\"; done; echo $foo; fi; rm -f NessusCAD*"
	expect		:	"[Vv]ersion"
#	severity    	: 	MEDIUM
#	quiet			:	YES
	dont_echo_cmd	:	YES
</custom_item>

<custom_item>
	system			:	"Linux"
	type			:	CMD_EXEC
	description		:	"Custom Asset Discovery Tomcat"
	cmd				:	"cd /tmp && foo=\'for bar in `timeout 30s find / -not -path \"/proc/*\" -not -path \"*/docker/*\" -type f -size -1M  -maxdepth 8 -name \"version.sh\" 2>/dev/null | grep tomcat`; do sh $bar; done\'; bash -c \"$foo\" > NessusCAD 2>/dev/null; grep -i \"Apache Tomcat\" NessusCAD  | awk -F \"/\"  \'{print $2}\' >NessusCAD1; for ver in `sort NessusCAD1 2>/dev/null | uniq`; do echo \"Tomcat Version: $ver\"; done; rm -f NessusCAD*"
	expect		:	"[Vv]ersion"
#	severity     	: 	MEDIUM
#	quiet			:	YES
	dont_echo_cmd	:	YES
</custom_item>

